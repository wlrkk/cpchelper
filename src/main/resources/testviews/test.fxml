<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.TextField?>

<?import javafx.geometry.Insets?>
<VBox fx:id="rootStage"
      maxHeight="-Infinity"
      maxWidth="-Infinity"
      minHeight="-Infinity"
      minWidth="-Infinity"
      prefHeight="400"
      prefWidth="600"
      xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.nanyue.cpchelper.controller.TestController">
        <VBox alignment="CENTER">
            <padding>
                <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
            </padding>
            <Label alignment="CENTER" fx:id="helloLabel" prefHeight="34.0" prefWidth="394.0" text="Hello World!">
                <styleClass>
                    <String fx:value="lbl" />
                    <String fx:value="lbl-primary" />
                </styleClass>
                <font>
                    <Font size="13.0" />
                </font>
            </Label>
            <HBox alignment="CENTER">
                <TextField alignment="CENTER" fx:id="nameField"  prefHeight="26.0" prefWidth="234.0">
                    <styleClass>
                        <String fx:value="text-info" />
                    </styleClass>
                </TextField>
            </HBox>
            <HBox alignment="CENTER" spacing="10.0">
                <Button alignment="CENTER" fx:id="helloButton" layoutX="347.0" layoutY="162.0" mnemonicParsing="false" onAction="#setHelloText" text="浏览">
                    <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="btn-default" />
                    </styleClass>
                </Button>
                <Button alignment="CENTER" fx:id="helloButton2" layoutX="347.0" layoutY="162.0" mnemonicParsing="false" onAction="#setHelloText" text="浏览">
                    <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="btn-default" />
                    </styleClass>
                </Button>
            </HBox>
        </VBox>
</VBox>
